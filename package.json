{
  "name": "doctor",
  "version": "1.0.0",
  "description": "Seba Backend",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "postinstall": "tsc",
    "dev": "nodemon --config nodemon.json src/app.ts",
    "watch-ts": "tsc -w",
    "build": "tsc -p .",
    "prettier": "prettier --write 'src/**/*.{ts,json}'",
    "lint": "eslint './**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "cloudinary": "^1.26.0",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "env-var": "^7.0.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.4",
    "nanoid": "^3.1.23",
    "numeric-code": "^1.0.4",
    "pino": "^6.12.0",
    "pino-pretty": "^5.1.1",
    "socket.io": "^4.1.3",
    "twilio": "^3.66.0",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/chalk": "^2.2.0",
    "@types/compression": "^1.7.1",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/mongoose": "^5.11.97",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.3.2",
    "@types/pino": "^6.3.9",
    "@types/twilio": "^3.19.3",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.13",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
